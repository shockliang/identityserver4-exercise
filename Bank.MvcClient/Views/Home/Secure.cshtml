@model TModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Secure</title>
</head>
<body>
<div>
    <h2>Secure</h2>
    <h3>User Claims</h3>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dt>@claim.Value</dt>
        }
    </dl>
</div>
</body>
</html>